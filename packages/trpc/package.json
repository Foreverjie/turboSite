{
    "name": "trpc-config",
    "version": "1.0.0",
    "description": "Shared tRPC configuration and utilities",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./server": "./src/server/index.ts",
        "./server/web": "./src/server/web.ts",
        "./client": "./src/client/index.ts",
        "./react": "./src/react/index.ts",
        "./src/*": "./src/*"
    },
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "clean": "rm -rf dist",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@trpc/client": "^10.45.2",
        "@trpc/server": "^10.45.2",
        "@trpc/react-query": "^10.45.2",
        "@tanstack/react-query": "^4.36.1",
        "@supabase/auth-helpers-nextjs": "^0.10.0",
        "superjson": "^2.2.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/react": "^18.2.79",
        "react": "^18.2.0",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "@tanstack/react-query": "^4.0.0"
    },
    "files": [
        "src/**/*"
    ]
