{
    "name": "trpc-config",
    "version": "1.0.0",
    "description": "Shared tRPC configuration and utilities",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./src/*": "./src/*"
    },
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "clean": "rm -rf dist",
        "type-check": "tsc --noEmit",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio"
    },
    "dependencies": {
        "@supabase/auth-helpers-nextjs": "^0.10.0",
        "@supabase/ssr": "^0.5.2",
        "@supabase/supabase-js": "^2.49.1",
        "@tanstack/react-query": "^4.36.1",
        "@trpc/client": "^10.45.2",
        "@trpc/react-query": "^10.45.2",
        "@trpc/server": "^10.45.2",
        "drizzle-orm": "^0.30.10",
        "postgres": "^3.4.3",
        "superjson": "^2.2.1",
        "trpc-panel": "^1.3.4",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/react": "^18.2.79",
        "drizzle-kit": "^0.20.0",
        "react": "^18.2.0",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "@tanstack/react-query": "^4.0.0",
        "react": "^18.0.0"
    },
    "files": [
        "src/**/*"
    ]
}